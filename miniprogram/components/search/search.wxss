.s-container {
    display: flex;
    align-items: center;
    margin-left: 20rpx;
    position: relative;
}

.s-bar {
    flex: 1;
    height: 68rpx;
    font-size: 28rpx;
    border-radius: 50px;
    background-color: #f5f5f5;
    padding-left: 60rpx;
}

.s-inbar {
    color: #999;
}

.s-btn {
    background-color: #d81e06;
    color: #fff;
    font-size: 24rpx;
    margin-left: 20rpx;
}

.searchIcon {
    position: absolute;
    top: 50%;
    transform: translate(16rpx, -50%);
    color: #9f9f9f;
}