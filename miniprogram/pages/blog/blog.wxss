.b-header {
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    background-color: #fff;
    height: 100rpx;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 99;
    box-sizing: border-box;
}

.icon-fabu {
    font-size: 48rpx;
}

.b-publish {
    width: 68rpx;
    height: 68rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.b-search {
    flex: 1;
}